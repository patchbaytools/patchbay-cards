# API Endpoints
# Production API URL for Patchbay Cards
NEXT_PUBLIC_PATCHBAY_API_PROD_URL=https://api.patchbay.xyz

# Staging API URL for Patchbay Cards
NEXT_PUBLIC_PATCHBAY_API_STAGING_URL=https://staging-api.patchbay.xyz

# Next.js Configuration
# Set to 'production' for production builds
NODE_ENV=development

# Optional: Analytics and Monitoring
# Vercel Analytics (if using Vercel)
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_analytics_id

# Optional: Performance Monitoring
# NEXT_PUBLIC_SPEED_INSIGHTS_ID=your_speed_insights_id
